NODE_ENV=development
PORT=5000
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/property_mapping
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRE=30d
JWT_COOKIE_EXPIRE=30

# Email configuration
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_EMAIL=your_mailtrap_email
SMTP_PASSWORD=your_mailtrap_password
FROM_EMAIL=noreply@propertymapping.com
FROM_NAME=PropertyMapping

# API keys
LOCATIONIQ_API_KEY=your_locationiq_api_key
CLEARBIT_API_KEY=your_clearbit_api_key
PEOPLE_DATA_LABS_API_KEY=your_people_data_labs_api_key
US_CENSUS_API_KEY=your_us_census_api_key

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# Rate limiting
RATE_LIMIT_WINDOW_MS=600000
RATE_LIMIT_MAX=300

# File upload limit (in bytes)
MAX_FILE_UPLOAD=1000000